{
	"name": "<%= name %>",
	"version": "1.0.0",
	"main": "build/server.js",
	"scripts": {
		"start": "babel-node src/server.js",
		"start-dev": "nodemon src/server.js --exec babel-node",
		"build": "babel src -d lib",
		"lint": "eslint src"
	},
	"babel": {
		"presets": [
			"es2015",
			"stage-0"
		]
	},
	"devDependencies": {
		"babel-cli": "^6.14.0",
		"babel-preset-es2015": "^6.14.0",
		"babel-preset-stage-0": "^6.5.0",
		"babel-register": "^6.14.0",
		"eslint": "^3.6.0",
		"eslint-config-airbnb-base": "^8.0.0",
		"eslint-plugin-import": "^1.16.0",
		"nodemon": "^1.10.2"
	},
	"dependencies": { <% if (mongo === 'yes') { %>
		"mongoose": "^4.6.1", <% } %> <% if (server === 'microsoft bot connector') { %>
		"botbuilder": "^3.1.1",
		"express": "^4.14.0",<% } %> <% if (server === 'kik') { %>
		"@kikinteractive/kik": "^2.0.10", <% } %>	<% if (server === 'slack') { %>
		"@slack/client": "^3.6.0", <% } %> <% if (server === 'messenger') { %>
		"request": "^2.75.0",
		"body-parser": "^1.15.2",
		"express": "^4.14.0", <% } %> <% if (example === 'yes') { %>
		"require-all": "^2.0.0", <% } %>
		"lodash": "^4.16.2"
	}
}
